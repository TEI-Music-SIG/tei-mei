      <xs:attribute name="rend" type="mei:data.TEXTRENDITION">
        <xs:annotation>
          <xs:documentation>captures the appearance of the element's contents.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="rotation" type="mei:data.DEGREES">
        <xs:annotation>
