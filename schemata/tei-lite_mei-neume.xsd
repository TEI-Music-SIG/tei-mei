      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.scoreDef.vis.attribute.page.leftmar">
    <xs:attribute name="page.leftmar" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>indicates the amount of whitespace at the left side of a rendered score page.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.scoreDef.vis.attribute.page.rightmar">
    <xs:attribute name="page.rightmar" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>indicates the amount of whitespace at the right side of a rendered score
            page.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.scoreDef.vis.attribute.page.panels">
    <xs:attribute name="page.panels" type="mei:data.PAGE.PANELS">
      <xs:annotation>
        <xs:documentation>indicates the number of logical pages to be rendered on a single physical
            page.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.scoreDef.vis.attribute.page.scale">
    <xs:attribute name="page.scale" type="mei:data.PGSCALE">
      <xs:annotation>
        <xs:documentation>indicates how the page should be scaled when rendered.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.scoreDef.vis.attribute.spacing.packexp">
    <xs:attribute name="spacing.packexp" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>describes a note's spacing relative to its time value.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.scoreDef.vis.attribute.spacing.packfact">
    <xs:attribute name="spacing.packfact" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>describes the note spacing of output.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.scoreDef.vis.attribute.spacing.staff">
    <xs:attribute name="spacing.staff" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>sets the minimum amount of space between staves in the same system.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.scoreDef.vis.attribute.spacing.system">
    <xs:attribute name="spacing.system">
      <xs:annotation>
        <xs:documentation>contains a space-separated pair of numbers describing the minimum and maximum amount
            of space between systems.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.scoreDef.vis.attribute.system.leftmar">
    <xs:attribute name="system.leftmar" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>describes the amount of whitespace at the left system margin relative to
            page.leftmar.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.scoreDef.vis.attribute.system.rightmar">
    <xs:attribute name="system.rightmar" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>describes the amount of whitespace at the right system margin relative to
            page.rightmar.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.scoreDef.vis.attribute.system.topmar">
    <xs:attribute name="system.topmar" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>describes the distance from page's top edge to the first system; used for first page
            only.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
