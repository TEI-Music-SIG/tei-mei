    <xs:attribute name="artic.ges" type="mei:data.ARTICULATIONS">
      <xs:annotation>
        <xs:documentation>records performed articulation that differs from the written value.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
