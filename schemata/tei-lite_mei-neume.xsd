    <xs:attribute name="source">
      <xs:annotation>
        <xs:documentation>contains a list of one or more pointers indicating sources
      supporting the given  intervention or interpretation.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction>
          <xs:simpleType>
            <xs:list>
              <xs:simpleType>
                <xs:union memberTypes="mei:data.pointer mei:data.pointer"/>
              </xs:simpleType>
            </xs:list>
          </xs:simpleType>
          <xs:minLength value="1"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
