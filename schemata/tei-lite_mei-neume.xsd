      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="mei:resp"/>
        <xs:group ref="mei:mei_model.nameLike"/>
        <xs:group ref="mei:mei_model.nameLike.agent"/>
      </xs:choice>
      <xs:attributeGroup ref="mei:mei_att.bibl.attributes"/>
      <xs:attributeGroup ref="mei:mei_att.common.attributes"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="revisionDesc">
    <xs:annotation>
      <xs:documentation>(revision description)  Container for information about alterations that have
        been made to an MEI file.
        </xs:documentation>
