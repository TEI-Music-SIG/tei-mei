      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.scoreDef.vis.attribute.optimize">
    <xs:attribute name="optimize" type="mei:data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>indicates whether staves without notes, rests, etc. should be displayed. When the
            value is 'true', empty staves are displayed.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.scoreDef.vis.attribute.page.height">
    <xs:attribute name="page.height" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>describes the physical height of the rendered output page.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.scoreDef.vis.attribute.page.width">
    <xs:attribute name="page.width" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>describes the physical width of the rendered output page.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.scoreDef.vis.attribute.page.units">
    <xs:attribute name="page.units" type="mei:data.PGUNITS">
      <xs:annotation>
        <xs:documentation>contains the real-world measurement units (inches, centimeters, millimeters) used to
            describe the rendered page height, width, and margins.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.scoreDef.vis.attribute.page.topmar">
    <xs:attribute name="page.topmar" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>indicates the amount of whitespace at the top of a rendered score page.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.scoreDef.vis.attribute.page.botmar">
    <xs:attribute name="page.botmar" type="xs:decimal">
      <xs:annotation>
