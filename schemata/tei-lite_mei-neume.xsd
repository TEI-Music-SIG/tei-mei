  <xs:attributeGroup name="mei_att.staffDef.vis.attribute.grid.show">
    <xs:attribute name="grid.show" type="mei:data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>determines whether to display guitar chord grids.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.staffDef.vis.attribute.layerscheme">
    <xs:attribute name="layerscheme" type="mei:data.LAYERSCHEME">
      <xs:annotation>
        <xs:documentation>indicates the number of layers and their stem directions.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.staffDef.vis.attribute.lines">
    <xs:attribute name="lines" type="xs:positiveInteger">
      <xs:annotation>
        <xs:documentation>indicates the number of staff lines.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.staffDef.vis.attribute.lines.color">
    <xs:attribute name="lines.color" type="mei:data.COLORS">
      <xs:annotation>
        <xs:documentation>captures the colors of the staff lines. The value is structured; that is, it should
            have the same number of space-separated RGB values as the number of lines indicated by
            the lines attribute. A line can be made invisible by assigning it the same RGB value as
            the background, usually white.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.staffDef.vis.attribute.lines.visible">
    <xs:attribute name="lines.visible" type="mei:data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>records whether all staff lines are visible.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.staffDef.vis.attribute.spacing">
