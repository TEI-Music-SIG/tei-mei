      </xs:sequence>
      <xs:attributeGroup ref="mei:mei_att.common.attributes"/>
      <xs:attributeGroup ref="mei:mei_att.bibl.attributes"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="source">
