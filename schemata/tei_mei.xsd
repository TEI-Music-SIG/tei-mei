      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.coordinated.attribute.lrx">
    <xs:attribute name="lrx" type="xs:nonNegativeInteger">
      <xs:annotation>
        <xs:documentation>indicates the lower-right corner x coordinate.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.coordinated.attribute.lry">
    <xs:attribute name="lry" type="xs:nonNegativeInteger">
      <xs:annotation>
        <xs:documentation>indicates the lower-left corner x coordinate.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.curvature.attributes">
    <xs:attributeGroup ref="ns1:mei_att.curvature.attribute.bezier"/>
    <xs:attributeGroup ref="ns1:mei_att.curvature.attribute.bulge"/>
    <xs:attributeGroup ref="ns1:mei_att.curvature.attribute.curvedir"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.curvature.attribute.bezier">
    <xs:attribute name="bezier">
      <xs:annotation>
        <xs:documentation>records the placement of Bezier control points as a series of space-separated xy
            coordinates, e.g., 19 45 -32 118.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.curvature.attribute.bulge">
    <xs:attribute name="bulge">
      <xs:annotation>
        <xs:documentation>describes a curve as a set of distance values above or below an imaginary line
            connecting the endpoints of the curve. The bulge attribute must contain one or more
            decimal values expressed in inter-line units.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.curvature.attribute.curvedir">
    <xs:attribute name="curvedir">
      <xs:annotation>
        <xs:documentation>describes a curve with a generic term indicating the direction of curvature.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="above">
            <xs:annotation>
              <xs:documentation>upward curve.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="below">
            <xs:annotation>
              <xs:documentation>downward curve.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.curverend.attributes">
    <xs:attributeGroup ref="ns1:mei_att.curverend.attribute.rend"/>
