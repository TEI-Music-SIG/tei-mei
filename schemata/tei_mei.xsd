      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.duration.musical.attributes">
    <xs:attributeGroup ref="ns1:mei_att.duration.musical.attribute.dur"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.duration.musical.attribute.dur">
    <xs:attribute name="dur" type="ns1:data.DURATION">
