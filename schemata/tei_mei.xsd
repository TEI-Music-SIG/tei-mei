    <xs:attributeGroup ref="ns1:mei_att.datable.attribute.notafter"/>
    <xs:attributeGroup ref="ns1:mei_att.datable.attribute.notbefore"/>
    <xs:attributeGroup ref="ns1:mei_att.datable.attribute.startdate"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.datable.attribute.enddate">
    <xs:attribute name="enddate" type="ns1:data.ISODATE">
      <xs:annotation>
        <xs:documentation>contains the end point of a date range in standard ISO form.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.datable.attribute.isodate">
    <xs:attribute name="isodate" type="ns1:data.ISODATE">
      <xs:annotation>
        <xs:documentation>provides the value of a textual date in standard ISO form.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.datable.attribute.notafter">
    <xs:attribute name="notafter" type="ns1:data.ISODATE">
      <xs:annotation>
        <xs:documentation>contains an upper boundary for an uncertain date in standard ISO form.</xs:documentation>
