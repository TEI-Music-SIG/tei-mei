  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.transposition.attribute.trans.diat">
    <xs:attribute name="trans.diat" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>records the amount of diatonic pitch shift, e.g., C to C♯ = 0, C to D♭ = 1,
            necessary to calculate the sounded pitch from the written one.
            </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.transposition.attribute.trans.semi">
