    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.stemmed.attribute.stem.len">
    <xs:attribute name="stem.len" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>encodes the stem length.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.stemmed.attribute.stem.pos">
    <xs:attribute name="stem.pos" type="ns1:data.STEMPOSITION">
      <xs:annotation>
        <xs:documentation>records the position of the stem in relation to the note head(s).</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.stemmed.attribute.stem.x">
    <xs:attribute name="stem.x" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>records the output x coordinate of the stem's attachment point.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.stemmed.attribute.stem.y">
    <xs:attribute name="stem.y" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>records the output y coordinate of the stem's attachment point.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.syltext.attributes">
    <xs:attributeGroup ref="ns1:mei_att.syltext.attribute.syl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.syltext.attribute.syl">
    <xs:attribute name="syl">
      <xs:annotation>
