      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.textstyle.attributes">
    <xs:attributeGroup ref="ns1:mei_att.textstyle.attribute.text.fam"/>
    <xs:attributeGroup ref="ns1:mei_att.textstyle.attribute.text.name"/>
    <xs:attributeGroup ref="ns1:mei_att.textstyle.attribute.text.size"/>
    <xs:attributeGroup ref="ns1:mei_att.textstyle.attribute.text.style"/>
    <xs:attributeGroup ref="ns1:mei_att.textstyle.attribute.text.weight"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.textstyle.attribute.text.fam">
    <xs:attribute name="text.fam" type="ns1:data.FONTFAMILY">
      <xs:annotation>
        <xs:documentation>provides a default value for the font family name of text (other than lyrics) when
            this information is not provided on the individual elements.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.textstyle.attribute.text.name">
    <xs:attribute name="text.name" type="ns1:data.FONTNAME">
      <xs:annotation>
        <xs:documentation>provides a default value for the font name of text (other than lyrics) when this
            information is not provided on the individual elements.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.textstyle.attribute.text.size">
    <xs:attribute name="text.size" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>provides a default value for the font size of text (other than lyrics) when this
            information is not provided on the individual elements.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.textstyle.attribute.text.style">
    <xs:attribute name="text.style" type="ns1:data.FONTSTYLE">
      <xs:annotation>
        <xs:documentation>provides a default value for the font style of text (other than lyrics) when this
            information is not provided on the individual elements.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.textstyle.attribute.text.weight">
    <xs:attribute name="text.weight" type="ns1:data.FONTWEIGHT">
      <xs:annotation>
        <xs:documentation>provides a default value for the font weight for text (other than lyrics) when this
            information is not provided on the individual elements.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.tiepresent.attributes">
    <xs:attributeGroup ref="ns1:mei_att.tiepresent.attribute.tie"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.tiepresent.attribute.tie">
    <xs:attribute name="tie" type="ns1:data.TIES">
      <xs:annotation>
        <xs:documentation>indicates that this element participates in a tie. If visual information about the
            tie needs to be recorded, then a &lt;tie&gt; element should be employed.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.timestamp.musical.attributes">
    <xs:attributeGroup ref="ns1:mei_att.timestamp.musical.attribute.tstamp"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.timestamp.musical.attribute.tstamp">
    <xs:attribute name="tstamp" type="ns1:data.BEAT">
      <xs:annotation>
        <xs:documentation>encodes the onset time in terms of musical time, i.e.,
            beats[.fractional_beat_part].</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.timestamp.performed.attributes">
    <xs:attributeGroup ref="ns1:mei_att.timestamp.performed.attribute.tstamp.ges"/>
    <xs:attributeGroup ref="ns1:mei_att.timestamp.performed.attribute.tstamp.real"/>
