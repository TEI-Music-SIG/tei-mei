            <zeroOrMore>
               <ref name="model.pLike"/>
            </zeroOrMore>
      
      
        
            <optional>
               <ref name="objectDesc"/>
            </optional>
            <optional>
               <ref name="handDesc"/>
            </optional>
            <optional>
               <ref name="typeDesc"/>
            </optional>
            <optional>
               <ref name="scriptDesc"/>
            </optional>
            <optional>
               <ref name="musicNotation"/>
            </optional>
            <optional>
               <ref name="decoDesc"/>
            </optional>
            <optional>
               <ref name="additions"/>
            </optional>
            <optional>
               <ref name="bindingDesc"/>
            </optional>
            <optional>
               <ref name="sealDesc"/>
            </optional>
            <optional>
               <ref name="accMat"/>
            </optional>
      
         </group>
         <ref name="att.global.attributes"/>
         <empty/>
      </element>
