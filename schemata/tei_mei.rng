   <define name="mei_componentList">
      <element name="componentList" ns="http://www.music-encoding.org/ns/mei">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Container for intellectual or physical component parts of a bibliographic entity.</a:documentation>
         <zeroOrMore>
            <ref name="mei_model.headLike"/>
