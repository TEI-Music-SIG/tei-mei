         <choice>
            <zeroOrMore>
               <choice>
                  <ref name="mei_model.graphicPrimitiveLike"/>
                  <ref name="mei_graphic"/>
               </choice>
            </zeroOrMore>
         </choice>
         <zeroOrMore>
            <ref name="mei_model.annotLike"/>
         </zeroOrMore>
         <ref name="mei_att.common.attributes"/>
         <ref name="mei_att.coordinated.attributes"/>
