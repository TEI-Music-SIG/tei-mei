        <zeroOrMore>
          <ref name="model.global"/>
        </zeroOrMore>
        <optional>
          <ref name="lem"/>
          <zeroOrMore>
            <ref name="model.global"/>
          </zeroOrMore>
          <optional>
            <ref name="wit"/>
            <zeroOrMore>
              <ref name="model.global"/>
            </zeroOrMore>
          </optional>
        </optional>
        <zeroOrMore>
          <choice>
            <group>
              <ref name="model.rdgLike"/>
              <zeroOrMore>
                <ref name="model.global"/>
              </zeroOrMore>
              <optional>
                <ref name="wit"/>
                <zeroOrMore>
                  <ref name="model.global"/>
                </zeroOrMore>
              </optional>
