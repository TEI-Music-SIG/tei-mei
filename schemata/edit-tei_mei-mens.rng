        <choice>
          <oneOrMore>
            <ref name="row"/>
            <zeroOrMore>
              <ref name="model.global"/>
            </zeroOrMore>
          </oneOrMore>
          <oneOrMore>
            <group>
              <ref name="model.graphicLike"/>
            </group>
            <zeroOrMore>
              <ref name="model.global"/>
            </zeroOrMore>
          </oneOrMore>
        </choice>
        <zeroOrMore>
          <group>
            <ref name="model.divBottom"/>
