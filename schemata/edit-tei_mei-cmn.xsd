  <xs:attributeGroup name="mei_att.cleffing.vis.attribute.clef.color">
    <xs:attribute name="clef.color" type="ns1:data.COLOR">
      <xs:annotation>
        <xs:documentation>describes the color of the clef.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.cleffing.vis.attribute.clef.visible">
