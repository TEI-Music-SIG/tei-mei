    <xs:attribute name="calendar" type="xs:NMTOKEN">
      <xs:annotation>
        <xs:documentation>indicates the calendar system to which a date belongs, for example, Gregorian, Julian, Roman, Mosaic, Revolutionary, Islamic, etc.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.canonical.attributes">
