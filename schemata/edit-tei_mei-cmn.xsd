  <xs:attributeGroup name="mei_att.clefshape.attributes">
    <xs:attributeGroup ref="ns1:mei_att.clefshape.attribute.shape"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.clefshape.attribute.shape">
    <xs:attribute name="shape" type="ns1:data.CLEFSHAPE">
